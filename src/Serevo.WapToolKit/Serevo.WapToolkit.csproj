<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net462</TargetFramework>
    <GenerateDocumentationFile>True</GenerateDocumentationFile>
    <SignAssembly>True</SignAssembly>
    <AssemblyOriginatorKeyFile>snkey.snk</AssemblyOriginatorKeyFile>
    <RepositoryUrl>https://github.com/serevo/storex-sdk</RepositoryUrl>
    <Title>Toolkit for Windows Application Package (MSIX)</Title>
    <Version>1.0.0-rc.3</Version>
    <Authors>SHINSEI ELECTRONICS</Authors>
    <Description>Toolkit for Windows Application Package (MSIX)</Description>
    <Copyright>© 2023 SHINSEI ELECTRONICS CO,.LTD.</Copyright>
    <PackageProjectUrl>https://github.com/serevo/wap-toolkit</PackageProjectUrl>
    <PackageTags>Desktop Bridge;MSIX;SettingsProvider;</PackageTags>
    <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
    <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="snkey.snk" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Windows.SDK.Contracts" Version="10.0.17134.1000" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="System.Configuration" />
  </ItemGroup>

</Project>
